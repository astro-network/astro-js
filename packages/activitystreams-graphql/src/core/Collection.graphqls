# https://www.w3.org/TR/activitystreams-vocabulary/#dfn-collection
interface Collection {
  _context:   [URI!]!
  id:         ID!
  type:       [CollectionType!]!
  url:        URI!
  items:      [LinkOrObject!]!
}

interface CollectionItemsCount {
  totalItems: NonNegativeInteger!
}

interface CollectionLinks {
  current:    LinkOrCollectionPage!
  first:      LinkOrCollectionPage!
  last:       LinkOrCollectionPage!
}
