{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist/env",
    "outDir": "./dist/env",

    "typeRoots": [],
    "types": [],

    "allowUnusedLabels": true,
    "alwaysStrict": true,
    "emitDeclarationOnly": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true
  },
  "files": [
    "src/env/api.ts",
    "src/env/crypto.ts",
    "src/env/global.ts",
    "src/env/index.ts",
    "src/env/optional-callbacks.ts",
    "src/env/required-callbacks.ts"
  ]
}
