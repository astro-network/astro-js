{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist/env",
    "outDir": "./dist/env",
    "typeRoots": [],
    "types": [],

    "allowUnusedLabels": true,
    "alwaysStrict": true,
    "emitDeclarationOnly": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true
  },
  "include": [
    "src/env/"
  ],
  "exclude": [
    "dist/env/"
  ]
}
